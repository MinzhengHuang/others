<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.others"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/others"
        android:label="@string/app_name"
        android:theme="@style/MMTheme.Basic">
        <activity
            android:name=".activity.OthersMainActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".countdowntimer.CountDownTimerActivity"></activity>
        <activity android:name=".activity.CircleIndicatorActivity"></activity>
        <activity android:name=".activity.QuantityViewActivity"></activity>
        <activity android:name=".activity.CircleProgressActivity"></activity>
        <activity android:name=".activity.JSBridgeActivity"></activity>
        <activity android:name=".activity.AndroidJSActivity"></activity>
        <activity android:name=".activity.IndexableListViewActivity"></activity>
        <activity android:name=".activity.AndroidCheckBoxActivity"></activity>
        <activity android:name=".activity.DateWheelActivity"></activity>
        <activity android:name=".activity.SmsValidateCodeActivity"></activity>
        <activity android:name=".activity.PickerViewActivity"></activity>
        <activity android:name=".pulldownlistview.PulldownListviewActivity" ></activity>
        <activity android:name=".activity.LoadingBarActivity" ></activity>
        <activity android:name=".activity.WeixinGroupIconActivity" ></activity>
    </application>

</manifest>